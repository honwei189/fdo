{
    "name": "honwei189/fdo",
    "description": "Fast database ORM (with or without) library for PHP.  Able to connect mutiple DB at the same time.  Laravel users can use this library to replace laravel eloquent or use for any type of PHP projects with composer structure",
    "authors": [
        {
            "name": "Gordon Lim",
            "email": "honwei189@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "honwei189/flayer": "^1.0",
        "php": ">=7.3.0"
    },
    "autoload": {
        "psr-4": {
            "honwei189\\fdo\\": "src/"
        },
        "classmap": [
            "fdo/src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "honwei189\\fdo\\fdoServiceProvider"
            ],
            "aliases": {
                "fdo": "honwei189\\fdo\\fdo"
            }
        },
        "installer-paths": {
            "./vendor/{$vendor}/honwei189/libs/{$name}": [
                "honwei189/fdo"
            ]
        }
    }
}

